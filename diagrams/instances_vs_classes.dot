digraph {
    ratio=0.56

    node [
        shape=box
        fontname="Helvetica Neue"
    ]

    edge [
        fontname="Helvetica Neue"
        fontsize=10
    ]

    object, Car[
        style=filled
        fillcolor="#001484"
        color = "#aaaaff"
        fontcolor=white
    ]

    object -> Car [label=" inherits from"]
    Car -> my_car [label=" instance of"]

    {rank = same; Car; my_car}
}